	rdfs:subClassOf
		fibo-sec-dbt-ab-pbs:PoolOfIndividualDebts ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ab-pbs:developsInto ;
			owl:someValuesFrom fibo-sec-dbt-ab-mbs:IssuedAndSecuritizedPoolOfIndividualDebts ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ab-pbs:lifecycleState ;
			owl:someValuesFrom fibo-bp-iss-dbti:InIssuance ;
		]
		;
	rdfs:label "completed debts pool"@en ;
	rdfs:comment "QName: NONE:CompletedDebtsPool" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BP/SecuritiesIssuance/DebtIssuance/> ;
