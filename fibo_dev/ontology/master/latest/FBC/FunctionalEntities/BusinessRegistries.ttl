<https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/>
	a owl:Ontology ;
	rdfs:label "Business Registries Ontology" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:" ;
	dct:abstract "This ontology extends the Registration Authorities ontology to define specific kinds of registries, such as business entity registries, registries for identifiers and codes of various sorts, and registries for financial institutions and intermediaries based on jurisdiction, who regulates them, and the services they provide." ;
	dct:license "http://opensource.org/licenses/MIT"^^xsd:anyURI ;
	sm:contentLanguage "http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI ;
	sm:copyright
		"Copyright (c) 2015-2019 EDM Council, Inc." ,
		"Copyright (c) 2015-2019 Object Management Group, Inc."
		;
	sm:dependsOn
		"https://spec.edmcouncil.org/fibo/ontology/BE/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/FinancialServicesEntities/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/FinancialProductsAndServices/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FND/"^^xsd:anyURI ,
		"https://www.omg.org/spec/LCC/"^^xsd:anyURI
		;
	sm:fileAbbreviation "fibo-fbc-fct-breg" ;
	sm:filename "BusinessRegistries.rdf" ;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/Corporations/Corporations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/LegalEntities/FormalBusinessOrganizations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/LegalEntities/LEIEntities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/FunctionalEntities/FinancialServicesEntities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/FunctionalEntities/RegistrationAuthorities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/ProductsAndServices/FinancialProductsAndServices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/AgentsAndPeople/Agents/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/ClassificationSchemes/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Codes/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/IdentifiersAndIndices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Law/Jurisdiction/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Organizations/FormalOrganizations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Addresses/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Countries/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/VirtualPlaces/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/> ,
		<https://www.omg.org/spec/LCC/Countries/CountryRepresentation/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:changeNote
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per FIBO 2.0 RFC primarily to loosen the constraints on address properties and better support standards including ISO 9362 (BIC codes), ISO 13616 (IBAN and BBAN codes), and ISO 17442 (the GLIEF LEI standard)." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FBC 1.1 RTF report." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified to generalize certain unions where they were no longer required, use the composite date datatype where appropriate, add individuals for entity expiration reason and validation level to better align with the GLEIF LEI data, and move international registration authorities, such as SWIFT, to a separate ontology for better modularity." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to eliminate deprecated elements." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to replace hasDefinition with isDefinedIn to clarify intent."
		;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-be-le-fbo:RegisteredAddress
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-fct-breg:RegistrationAddress ;
	.

fibo-be-le-lp:LegalEntity
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityExpirationReason ;
			owl:onClass fibo-fbc-fct-breg:EntityExpirationReason ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityStatus ;
			owl:onClass fibo-fbc-fct-breg:EntityStatus ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityExpirationDate ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			owl:onDataRange fibo-fnd-dt-fd:CombinedDateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-fpas:precedes ;
			owl:onClass fibo-be-le-lp:LegalEntity ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
			owl:onClass fibo-fbc-fct-breg:NorthAmericanIndustryClassificationSystemCode ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasHeadquartersAddress ;
			owl:onClass fibo-fbc-fct-breg:RegistrationAddress ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
			owl:onClass fibo-fbc-fct-breg:StandardIndustrialClassificationCode ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	.

fibo-fbc-fct-breg:ActiveStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityStatus
		;
	rdfs:label "active status" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:ActiveStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "as of the last report or update, the entity was legally registered and operating" ;
	fibo-fnd-rel-rel:hasTag "ACTIVE" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:AnnulledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "annulled status" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:AnnulledStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was determined to be erroneous or invalid after issuance" ;
	fibo-fnd-rel-rel:hasTag "ANNULLED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegisterIdentifier
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-id:Identifier ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-aap-agt:identifies ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistry ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:isRegisteredBy ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
		]
		;
	rdfs:label "business register identifier" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:BusinessRegisterIdentifier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an identifier that uniquely identifies a business register, such as a register identified by the Global Legal Entity Identifier Foundation (GLEIF) registration authorities list" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistrationAuthority
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistrationAuthority ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:registers ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:manages ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistry ;
		]
		;
	rdfs:label "business registration authority" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:BusinessRegistrationAuthority" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registration authority that is responsible for maintaining a registry of business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A business registry may include any government-managed registry for registering a business, such as a state department of corporations in the US, as well as other registries such as a local operating unit (LOU) for registration of legal entity identifiers (LEIs).  Any sanctioned registration authority as defined by the Registration Authorities List, published by GLEIF, is a business registration authority in this sense." ;
	.

fibo-fbc-fct-breg:BusinessRegistry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistryName ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistryEntry ;
		]
		;
	rdfs:label "business registry" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:BusinessRegistry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registry for registering and maintaining information about business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistryEntry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistrationStatus ;
			owl:onClass fibo-fbc-fct-breg:RegistrationStatus ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistrationDate ;
			owl:someValuesFrom fibo-fnd-dt-fd:CombinedDateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		]
		;
	rdfs:label "business registry entry" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:BusinessRegistryEntry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an entry in a business registry" ;
	.

fibo-fbc-fct-breg:CancelledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "cancelled status" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:CancelledStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was abandoned prior to issuance" ;
	fibo-fnd-rel-rel:hasTag "CANCELLED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:DuplicateStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "duplicate status" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:DuplicateStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was determined to be a duplicate registration of the same entity as another registration; duplicate status is assigned to the non-surviving registration (i.e. the identifier that should no longer be used)" ;
	fibo-fnd-rel-rel:hasTag "DUPLICATE" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReason
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-cd:CodeElement ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "entity expiration reason" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:EntityExpirationReason" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "code for the reason that a legal entity ceased to exist and/or operate" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReasonCorporateAction
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityExpirationReason
		;
	rdfs:label "entity expiration reason - corporate action" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:EntityExpirationReasonCorporateAction" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an expiration reason indicating that an entity was acquired or merged with another entity" ;
	fibo-fnd-rel-rel:hasTag "CORPORATE_ACTION" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReasonDissolved
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityExpirationReason
		;
	rdfs:label "entity expiration reason - disolved" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:EntityExpirationReasonDissolved" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an expiration reason indicating that an entity ceased to operate" ;
	fibo-fnd-rel-rel:hasTag "DISSOLVED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReasonOther
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityExpirationReason
		;
	rdfs:label "entity expiration reason - other" ;
	rdfs:comment "QName: fibo-fbc-fct-breg:EntityExpirationReasonOther" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
