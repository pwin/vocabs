fibo-loan-typ-mtg:MortgageLoan
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-typ-sec:CollateralizedSecuredLoan ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-typ-mtg:hasBorrowingParty ;
			owl:someValuesFrom fibo-loan-typ-mtg:MortgageBorrower ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-typ-mtg:hasLendingParty ;
			owl:someValuesFrom fibo-loan-typ-mtg:MortgageLender ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-typ-mtg:hasMIGProvider ;
			owl:someValuesFrom fibo-loan-loan-gty:LoanMIGProvider ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-typ-mtg:hasParty ;
			owl:someValuesFrom fibo-loan-loan-pty:LoanServicer.1 ;
		]
		;
	rdfs:label "mortgage loan"@en ;
	rdfs:comment "QName: NONE:MortgageLoan" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanTypes/MortgageLoans/> ;
	skos:definition "A loan securitized against some real estate."@en ;
	skos:editorialNote "Definition probably incomplete. Mortgage is not only securitized on the real estate but is used to fund the purchase of that real estate. Not sure of the best form of wording."@en ;
	.

