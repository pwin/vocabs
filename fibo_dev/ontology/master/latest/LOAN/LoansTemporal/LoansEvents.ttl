@base <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoansEvents/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix fibo-be-le-lp: <https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/> .
@prefix fibo-fnd-acc-cur: <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> .
@prefix fibo-fnd-dt-fd: <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/> .
@prefix fibo-fnd-dt-oc: <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/Occurrences/> .
@prefix fibo-fnd-law-cor: <https://spec.edmcouncil.org/fibo/ontology/FND/Law/LegalCore/> .
@prefix fibo-fnd-law-jur: <https://spec.edmcouncil.org/fibo/ontology/FND/Law/Jurisdiction/> .
@prefix fibo-fnd-pas-pas: <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/> .
@prefix fibo-fnd-pas-psch: <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/PaymentsAndSchedules/> .
@prefix fibo-fnd-rel-rel: <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/> .
@prefix fibo-fnd-utl-av: <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/> .
@prefix fibo-loan-loan-col: <https://spec.edmcouncil.org/fibo/ontology/LOAN/Loans/LoansCollateral/> .
@prefix fibo-loan-loan-pty: <https://spec.edmcouncil.org/fibo/ontology/LOAN/Loans/LoansParties/> .
@prefix fibo-loan-tem-col: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoanCollateralTemporal/> .
@prefix fibo-loan-tem-ev: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> .
@prefix fibo-loan-tem-ltv: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansLTV/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sm: <http://www.omg.org/techprocess/ab/SpecificationMetadata/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/>
	a owl:Ontology ;
	rdfs:label "LoansEvents"@en ;
	rdfs:comment "QName: NONE:" ;
	dct:abstract """This ontology defines a wide range of events relating to loans. These include legal proceedings, prepayments, events affecting the collateral such as valuation, purchase and sale and details of many of these concepts. 
		Note that most of this material was derived from an earlier activity with IBM Research, and are substantively based on a logical data model from that exercise.""" ;
	dct:license "http://opensource.org/licenses/MIT"^^xsd:anyURI ;
	sm:copyright """Copyright (c) 2015-2018 EDM Council, Inc.
Copyright (c) 2015-2018 Object Management Group, Inc.""" ;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/CurrencyAmount/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/Occurrences/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Law/Jurisdiction/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Law/LegalCore/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Parties/Parties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/PlacesExt/RealEstate/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/PaymentsAndSchedules/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/ProductsAndServices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/Loans/LoansCollateral/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/Loans/LoansParties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoanCollateralTemporal/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoansLTV/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoansEvents/> ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional ;
	.

fibo-fnd-pas-psch:Payment
	a owl:Class ;
	rdfs:subClassOf fibo-loan-tem-ev:ValueChangingActivity ;
	.

fibo-loan-tem-ev:CountyCourt
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-law-cor:CourtOfLaw ;
	rdfs:label "county court"@en ;
	rdfs:comment "QName: NONE:CountyCourt" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:CountyCourtJudgment
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:LegalProceeding ,
		fibo-loan-tem-ev:ValueChangingActivity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:deliveredBy ;
			owl:someValuesFrom fibo-loan-tem-ev:CountyCourt ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isAgainst ;
			owl:someValuesFrom fibo-be-le-lp:LegalPerson ;
		]
		;
	rdfs:label "county court judgment"@en ;
	rdfs:comment "QName: NONE:CountyCourtJudgment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "Satisfaction code associated with a county court judgement can have values satisfied/unsatisfied. This is a value changing event."@en ;
	.

fibo-loan-tem-ev:DateOfPurchase
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:ExplicitDate ;
	rdfs:label "date of purchase"@en ;
	rdfs:comment "QName: NONE:DateOfPurchase" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:DefaultProceedingTypeSelection
	a owl:Class ;
	rdfs:label "default proceeding type selection"@en ;
	rdfs:comment "QName: NONE:DefaultProceedingTypeSelection" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "(1) Default amount (2) Property Sale (3) Loss amount (4) Recoveries (5) Professional Negligence"@en ;
	.

fibo-loan-tem-ev:InformativeEvent
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
	rdfs:label "informative event"@en ;
	rdfs:comment "QName: NONE:InformativeEvent" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "An event which reflects some change in information about something."@en ;
	fibo-fnd-utl-av:explanatoryNote "This is not an event which has any impact on any economic facts. Example: A change of name of a corporation is an informative event. Court proceedings that impose restricitons on paying any further advance on a loan is an informative event."@en ;
	.

fibo-loan-tem-ev:LegalProceeding
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isAgainst ;
			owl:someValuesFrom fibo-be-le-lp:LegalPerson ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:takesPlaceUnder ;
			owl:someValuesFrom fibo-fnd-law-jur:Jurisdiction ;
		]
		;
	rdfs:label "legal proceeding"@en ;
	rdfs:comment "QName: NONE:LegalProceeding" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:LoanDefaultProceeding
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:LegalProceeding ,
		fibo-loan-tem-ev:ValueChangingActivity
		;
	rdfs:label "loan default proceeding"@en ;
	rdfs:comment "QName: NONE:LoanDefaultProceeding" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "[no definition] Further Review This is typically part of mortgagte servicing. THere would typically be a whole department dealing with this. Dealing with default, helping borrowers make payment Collections Default admin Foreclosure Reselling All dealt with by several sub departments. This requires subject matter experts in this area. 1. scoping Identify default as a possible state. This hands off to other business processes. Once you get into the default scenario we are talking about a proces that is going to fall into place over a period of time. The bank works out what to do with the default scenario, e.g. whether it restructures, forecloses, seeks restitution from the security (collateral). It does nto help us to understand the structure of the loan, rather tha consequences of the loan. If we were to further explore the default detail we would bring in other SMEs. And we would have to model a process flow. 1.1 impact on the pool of an MBS Loan Default Proceeding (special ase of legal thing) is an aspect of Default Management / Administratoin. there is also the State of the Loan. Sale / something / fulfilment / fiunduing / approved = servicing mode. Something happens (non payment) =&gt; Default Grace Period followed by negotiation. Some threshold whereby after a given amount of delinquency it needs to go into some other process moving towards foreclosure. Do State Diagram. Stages of loan."@en ;
	.

fibo-loan-tem-ev:MortgageLending
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:PropertyRelatedActivity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:determines ;
			owl:someValuesFrom fibo-loan-tem-ltv:OriginalLTV ;
		]
		;
	rdfs:label "mortgage lending"@en ;
	rdfs:comment "QName: NONE:MortgageLending" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:NonPayment
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:ValueChangingActivity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isNot ;
			owl:someValuesFrom fibo-fnd-pas-psch:Payment ;
		]
		;
	rdfs:label "non payment"@en ;
	rdfs:comment "QName: NONE:NonPayment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	owl:disjointWith fibo-fnd-pas-psch:Payment ;
	.

fibo-loan-tem-ev:NonValueChangingActivity
	a owl:Class ;
	rdfs:label "non value changing activity"@en ;
	rdfs:comment "QName: NONE:NonValueChangingActivity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	owl:disjointWith fibo-loan-tem-ev:ValueChangingActivity ;
	.

fibo-loan-tem-ev:Prepayment
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-pas-psch:Payment ;
	rdfs:label "prepayment"@en ;
	rdfs:comment "QName: NONE:Prepayment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:PropertyBuyer
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-pas:Buyer ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isBuyer ;
			owl:someValuesFrom fibo-loan-tem-ev:PropertyPurchaseAndSale ;
		]
		;
	rdfs:label "property buyer"@en ;
	rdfs:comment "QName: NONE:PropertyBuyer" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:PropertyCollateralValuation
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-loan-col:isOf ;
			owl:allValuesFrom fibo-loan-loan-col:LoanPropertyCollateral ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
			owl:someValuesFrom fibo-loan-loan-pty:AVMProvider ;
		]
		;
	rdfs:label "property collateral valuation"@en ;
	rdfs:comment "QName: NONE:PropertyCollateralValuation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "A valuation of the collateral Real Property to support the Property collateral Arrangement"@en ;
	skos:editorialNote "Review archetype in light of non typical relationships. Valuation has some formal, financial and legal standing over and above being an Activity. Hence parties etc. also then relate to the valuation aactivity (see adjacent)."@en ;
	.

fibo-loan-tem-ev:PropertyLendersValuationTypeSelection
	a owl:Class ;
	rdfs:label "property lenders valuation type selection"@en ;
	rdfs:comment "QName: NONE:PropertyLendersValuationTypeSelection" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:PropertyPurchaseAndSale
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:PropertyRelatedActivity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasDate ;
			owl:someValuesFrom fibo-loan-tem-ev:DateOfPurchase ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:resultsIn ;
			owl:someValuesFrom fibo-loan-tem-ev:PurchasePrice ;
		]
		;
	rdfs:label "property purchase and sale"@en ;
	rdfs:comment "QName: NONE:PropertyPurchaseAndSale" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "The sale and purchase of real estate."@en ;
	.

fibo-loan-tem-ev:PropertyRelatedActivity
	a owl:Class ;
	rdfs:label "property related activity"@en ;
	rdfs:comment "QName: NONE:PropertyRelatedActivity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "not sure if this is useful as a common class. Used for grouping of new activity terms around mortgage lending and propery purchase for now"@en ;
	.

fibo-loan-tem-ev:PropertyValuation
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-ev:PropertyRelatedActivity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:resultsIn.1 ;
			owl:someValuesFrom fibo-loan-tem-ev:PropertyValuationHistoricalInformation ;
		]
		;
	rdfs:label "property valuation"@en ;
	rdfs:comment "QName: NONE:PropertyValuation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "The activity whereby some real estats is valued by some agent, for some purpose (for example, for the purpose of granting or denoying a loan)."@en ;
	fibo-fnd-utl-av:explanatoryNote 'The activity of valuation of the propetry, which is carried out prior to setting up the loan, gives rise to "Property Valuation" information which is retained through the life of the loan.'@en ;
	.

fibo-loan-tem-ev:PropertyValuationHistoricalInformation
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-loan-tem-col:isOf ;
		owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/RealEstate> ;
	] ;
	rdfs:label "property valuation historical information"@en ;
	rdfs:comment "QName: NONE:PropertyValuationHistoricalInformation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "Valuation of the property at some point in time."@en ;
	.

fibo-loan-tem-ev:PropertyValuer
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-loan-tem-ev:carriesOut ;
		owl:someValuesFrom fibo-loan-tem-ev:PropertyValuation ;
	] ;
	rdfs:label "property valuer"@en ;
	rdfs:comment "QName: NONE:PropertyValuer" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "The party which carries out the valluation of the property for the purpose of a mortgage loan. Further notes: Also: See Event State Analysis at Fannie Mae, for core processes, as partof the enterprise data architecture transformation, e.g. ESA for single family and multi family loans."@en ;
	.

fibo-loan-tem-ev:PurchasePrice
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "purchase price"@en ;
	rdfs:comment "QName: NONE:PurchasePrice" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	.

fibo-loan-tem-ev:ValueChangingActivity
	a owl:Class ;
	rdfs:label "value changing activity"@en ;
	rdfs:comment "QName: NONE:ValueChangingActivity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	skos:definition "Some event or activity which changes the value of something. Examples: A value changing event can be for instance (in securities), a declaration of a dividend. This is an event which changes the value of something. CAE that are in FIBIM are value changing events. Changes to principal on any loan is a value changing event."@en ;
	.

fibo-loan-tem-ev:borrowerCCJudgementDate
	a owl:ObjectProperty ;
	rdfs:label "borrower c c judgement date"@en ;
	rdfs:comment "QName: NONE:borrowerCCJudgementDate" ;
	rdfs:domain fibo-loan-tem-ev:CountyCourtJudgment ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range fibo-fnd-dt-fd:Date ;
	skos:definition "Undefined PoC SDM Notes: Date this CCJ or equivalent was registered against the associated borrower regardless of satisfied or not"@en ;
	.

fibo-loan-tem-ev:borrowerCCJudgementSatisfaction
	a owl:DatatypeProperty ;
	rdfs:label "borrower c c judgement satisfaction"@en ;
	rdfs:comment "QName: NONE:borrowerCCJudgementSatisfaction" ;
	rdfs:domain fibo-loan-tem-ev:CountyCourtJudgment ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range xsd:string ;
	skos:definition "Undefined PoC SDM Notes: Satisfaction codes: 1 = this borrower County Court judgment was satisfied at time of underwriting; 2 = this borrower County Court judgment was unsatisfied at time of underwriting; Required that number of satisfied and unsatisfied CCJ's for a borrower can be determined from this data."@en ;
	.

fibo-loan-tem-ev:borrowerCCJudgementValueAmount
	a owl:ObjectProperty ;
	rdfs:label "borrower c c judgement value amount"@en ;
	rdfs:comment "QName: NONE:borrowerCCJudgementValueAmount" ;
	rdfs:domain fibo-loan-tem-ev:CountyCourtJudgment ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
	skos:definition "Undefined PoC SDM Notes: Total value of CCJs or equivalent recorded against the primary borrower that were satisfied / unsatsified (at time of underwriting ) is computed by selecting the satisfaction code value of interest and aggregating against all borrower CC Judgements &lt;/p&gt;"@en ;
	.

fibo-loan-tem-ev:carriesOut
	a owl:ObjectProperty ;
	rdfs:label "carries out"@en ;
	rdfs:comment "QName: NONE:carriesOut" ;
	rdfs:domain fibo-loan-tem-ev:PropertyValuer ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range fibo-loan-tem-ev:PropertyValuation ;
	.

fibo-loan-tem-ev:courtName
	a owl:DatatypeProperty ;
	rdfs:label "court name"@en ;
	rdfs:comment "QName: NONE:courtName" ;
	rdfs:domain fibo-loan-tem-ev:CountyCourt ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range xsd:string ;
	skos:definition "The formal name of the court."@en ;
	.

fibo-loan-tem-ev:deliveredBy
	a owl:ObjectProperty ;
	rdfs:label "delivered by"@en ;
	rdfs:comment "QName: NONE:deliveredBy" ;
	rdfs:domain fibo-loan-tem-ev:CountyCourtJudgment ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range fibo-loan-tem-ev:CountyCourt ;
	.

fibo-loan-tem-ev:determines
	a owl:ObjectProperty ;
	rdfs:label "determines"@en ;
	rdfs:comment "QName: NONE:determines" ;
	rdfs:domain fibo-loan-tem-ev:MortgageLending ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> ;
	rdfs:range fibo-loan-tem-ltv:OriginalLTV ;
	.

