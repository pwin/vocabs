fibo-md-derx-eto:refersTo.1
	a owl:ObjectProperty ;
	rdfs:label "refers to"@en ;
	rdfs:comment "QName: NONE:refersTo.1" ;
	rdfs:domain fibo-md-derx-eto:OptionsVega ;
