	rdfs:comment "QName: fibo-der-rtd-irswp:SwapStreamInterestPayment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
