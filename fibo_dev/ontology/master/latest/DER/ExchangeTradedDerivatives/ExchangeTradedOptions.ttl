fibo-der-etd-std:DerivativesListing
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-fnd-rel-rel:isProvidedBy ;
		owl:allValuesFrom [
			a owl:Class ;
			owl:unionOf (
				fibo-der-etd-std:DerivativesExchange
				fibo-der-etd-eto:OptionsClearingEntity
			) ;
		] ;
	] ;
	.

fibo-der-etd-eto:BondOption
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-ass-ass:DebtDerivativeContract ,
		fibo-der-etd-eto:OptionContractOnFutures ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
			owl:someValuesFrom fibo-der-etd-eto:BondOptionUnderlying ;
		]
		;
	rdfs:label "bond option"@en ;
	rdfs:comment "QName: NONE:BondOption" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "An option on a traded futures contract for a Bond. When exercised, the holder exercises the right to buy the future on a Bond, s/he does not exercise the right to buy a bond."@en ;
	fibo-fnd-utl-av:explanatoryNote "For example an option on a Treasury Bill is an option on a T Bill Future, not on a T Bill itself. Hence these are defined as a kind of futures option."@en ;
	.

fibo-der-etd-eto:BondOptionUnderlying
	a owl:Class ;
	rdfs:subClassOf fibo-der-etd-eto:OptionOnFuturesUnderlying ;
	rdfs:label "bond option underlying"@en ;
	rdfs:comment "QName: NONE:BondOptionUnderlying" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "The underlying on a Bond Option, which is the corresponding Bond Future contract on the same exchange."@en ;
	fibo-fnd-utl-av:explanatoryNote "This is the underlying in the sense that when the option is exercised the investor takes delivery of the corresponding futures contract."@en ;
	.

fibo-der-etd-eto:CommoditiesOption
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-com-ctr:CommodityDerivativeContract ,
		fibo-der-etd-eto:OptionContractOnFutures ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
			owl:someValuesFrom fibo-der-etd-eto:CommoditiesOptionUnderlying ;
		]
		;
	rdfs:label "commodities option"@en ;
	rdfs:comment "QName: NONE:CommoditiesOption" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "An option to buy or sell the future on some underlying commodity. The exercise is always to buy the underlying asset i.e. the future instrument NOT the underlying goods."@en ;
	.

fibo-der-etd-eto:CommoditiesOptionUnderlying
	a owl:Class ;
	rdfs:subClassOf fibo-der-etd-eto:OptionOnFuturesUnderlying ;
	rdfs:label "commodities option underlying"@en ;
	rdfs:comment "QName: NONE:CommoditiesOptionUnderlying" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "The underlying on a Commodities Option, which is the corresponding Commodity Future contract on the same exchange."@en ;
	fibo-fnd-utl-av:explanatoryNote "This is the underlying in the sense that when the option is exercised the investor takes delivery of the corresponding futures contract."@en ;
	.

fibo-der-etd-eto:CurrencyOptionExerciseTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-etd-eto:OptionInstrumentExerciseTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:delivery ;
			owl:someValuesFrom fibo-fnd-txn-sup:NegotiableCash ;
		]
		;
	rdfs:label "currency option exercise terms"@en ;
	rdfs:comment "QName: NONE:CurrencyOptionExerciseTerms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "Contractual terms defining when or under what circumstances an currency option is to be exercised."@en ;
	fibo-fnd-utl-av:explanatoryNote "Also includes terms defining delivery when the contract is exercised. Delivery for currency options is by way of cash."@en ;
	.

fibo-der-etd-eto:EquityIndexOptionUnderlying
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:UnderlyingOnOption ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:someValuesFrom fibo-ind-mkt-bas:EquityReferenceIndex ;
		]
		;
	rdfs:label "equity index option underlying"@en ;
	rdfs:comment "QName: NONE:EquityIndexOptionUnderlying" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "The inderlying of the equity index option, which is a reference index consisting of shares."@en ;
	skos:editorialNote "Review session note 16 Dec 2009: Nature of the \"Underlying\" as a class here: Somebody takes a defined Pf of those at that time. Modeling response: this describes a different sort of equity option, one based on a locally defined basket of equities. This has not been modeled. An equity index as defined here and in the Indices and Indicators part of the model is a published figure, out out by an index publisher. The composition of the basiet underkying that index is a matter for the publisher and not for the Options Contract. It sounds as though a locally defined basket may be used as another kind of Underlying, and may be loosely referred to as an index option, however that's not the term modeled here."@en ;
	.

fibo-der-etd-eto:EquityOption
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-ass-ass:EquityDerivativeContract ,
		fibo-der-etd-eto:ExchangeTradedOptionContract ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
			owl:someValuesFrom fibo-der-etd-eto:EquityOptionUnderlying ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ex:hasExerciseTerms ;
			owl:someValuesFrom fibo-der-etd-eto:StockOptionExerciseTerms ;
		]
		;
	rdfs:label "equity option"@en ;
	rdfs:comment "QName: NONE:EquityOption" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "Equity options are the most common type of equity derivative. They provide the right, but not the obligation to trade a quantity of stock at a set price at a future time."@en ;
	.

fibo-der-etd-eto:EquityOptionUnderlying
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:UnderlyingOnOption ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:someValuesFrom fibo-sec-eq-eq:ExchangeSpecificShare ;
		]
		;
	rdfs:label "equity option underlying"@en ;
	rdfs:comment "QName: NONE:EquityOptionUnderlying" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "The equity security that must be delivered when the option contract is exercised."@en ;
	skos:editorialNote "From SME Review 16 Dec 2009: Equity Option uses consolidated market data feed for the price of the underlying. so this is specified by for instance the national market system that consolidates the market information, so everyone is looking at the same Last Trade information. See Equity diagram where we have \"Composite\" market consisting of a number of markets. The Equity is normally not traded on the options exchange. But they will still attach to a market even if it's not their own. Further research: national market systems (US, what about the rest?)"@en ;
	.

fibo-der-etd-eto:ExchangeTradedEquityIndexOption
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-ass-ass:EquityDerivativeContract ,
		fibo-der-etd-eto:ExchangeTradedOptionContract ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
			owl:someValuesFrom fibo-der-etd-eto:EquityIndexOptionUnderlying ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ex:hasExerciseTerms ;
			owl:someValuesFrom fibo-der-etd-eto:IndexOptionExerciseTerms ;
		]
		;
	rdfs:label "exchange traded equity index option"@en ;
	rdfs:comment "QName: NONE:ExchangeTradedEquityIndexOption" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "Equity Index options offer the investor an opportunity to either capitalize on an expected market move or to protect holdings in the underlying instruments. The difference is that the underlying instruments are indexes."@en ;
	fibo-fnd-utl-av:explanatoryNote "These indexes can reflect the characteristics of either the broad equity market as a whole or specific industry sectors within the marketplace."@en ;
	.

fibo-der-etd-eto:ExchangeTradedOptionContract
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:Option ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:hasFeature ;
			owl:onClass fibo-der-der-opt:OptionBarrierFeatureTerms ;
			owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:definesOptional ;
			owl:someValuesFrom fibo-der-etd-eto:ExchangeTradedOptionTransaction ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:hasSeller ;
			owl:someValuesFrom fibo-der-etd-eto:TradedOptionPrincipal ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:hasContractDuration ;
			owl:someValuesFrom fibo-fnd-dt-fd:ExplicitDuration ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:hasCounterparty ;
			owl:someValuesFrom fibo-der-etd-eto:TradedOptionsCounterparty ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:hasPriceDeterminationMethod ;
			owl:someValuesFrom fibo-der-etd-std:DerivativesPriceDeterminationMethod ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-gty:hasGuarantor ;
			owl:someValuesFrom fibo-der-etd-eto:OptionGuarantor ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ex:hasExerciseTerms ;
			owl:someValuesFrom fibo-der-etd-eto:OptionInstrumentExerciseTerms ;
		]
		;
	rdfs:label "exchange traded option contract"@en ;
	rdfs:comment "QName: NONE:ExchangeTradedOptionContract" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	.

fibo-der-etd-eto:ExchangeTradedOptionTransaction
	a owl:Class ;
	rdfs:subClassOf fibo-der-der-opt:OptionalUnderlyingTransaction ;
	rdfs:label "exchange traded option transaction"@en ;
	rdfs:comment "QName: NONE:ExchangeTradedOptionTransaction" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	.

fibo-der-etd-eto:ForeignCurrencyOption
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-etd-eto:ExchangeTradedOptionContract ,
		fibo-der-fx-ctr:ForeignExchangeDerivativeContract ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-bsc:hasUnderlier ;
			owl:someValuesFrom fibo-der-etd-eto:FxOptionUnderlying ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ex:hasExerciseTerms ;
			owl:someValuesFrom fibo-der-etd-eto:CurrencyOptionExerciseTerms ;
		]
		;
	rdfs:label "foreign currency option"@en ;
	rdfs:comment "QName: NONE:ForeignCurrencyOption" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/ExchangeTradedDerivatives/ExchangeTradedOptions/> ;
	skos:definition "An option on a currency, based on the spot exchange rate of that currency to the currency of the security itself."@en ;
	skos:editorialNote "Definition needed - this is just a place holder. Actually, this would also have a Currency Future as the Underlying Action: change this. Definition:Review"@en ;
	.

fibo-der-etd-eto:FutureOptionsExerciseTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-etd-eto:OptionInstrumentExerciseTerms ,
		fibo-sec-dbt-ex:AmericanExerciseTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-etd-eto:delivery ;
			owl:someValuesFrom fibo-der-etd-fut:FutureInstrument ;
		]
		;
	rdfs:label "future options exercise terms"@en ;
	rdfs:comment "QName: NONE:FutureOptionsExerciseTerms" ;
