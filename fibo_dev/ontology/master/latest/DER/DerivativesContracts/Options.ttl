	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	.

fibo-der-der-opt:LookbackObservableValueDuringLookbackPeriod
	a owl:Class ;
	rdfs:subClassOf fibo-der-der-opt:LookbackObservableValue ;
	rdfs:label "lookback observable value during lookback period"@en ;
	rdfs:comment "QName: NONE:LookbackObservableValueDuringLookbackPeriod" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	.

fibo-der-der-opt:LookbackStrikeObservable
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:ObservableUnderlier ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:someValuesFrom fibo-fnd-math-math:TimedParameter ;
		]
		;
	rdfs:label "lookback strike observable"@en ;
	rdfs:comment "QName: NONE:LookbackStrikeObservable" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	skos:definition "The agreed parameter which is used in the formula for fixed OR floating lookback."@en ;
	.

fibo-der-der-opt:LookbackStrikeTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:OptionStrikeTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:hasSchedule ;
			owl:someValuesFrom fibo-der-der-opt:FloatingLookbackStrikeSchedule ;
