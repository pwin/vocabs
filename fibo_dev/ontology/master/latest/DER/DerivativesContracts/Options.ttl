	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:StrikeFormula ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:hasExpression ;
			owl:someValuesFrom fibo-der-der-opt:FixedLookbackStrikeFormulaExpression ;
		]
		;
	rdfs:label "fixed lookback strike formula"@en ;
	rdfs:comment "QName: NONE:FixedLookbackStrikeFormula" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	.

fibo-der-der-opt:FixedLookbackStrikeFormulaExpression
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:LookbackFormulaExpression ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:subtracts ;
			owl:someValuesFrom fibo-der-der-opt:ObservableBestValue ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:subtractsFrom ;
			owl:someValuesFrom fibo-der-der-opt:FixedStrikeAmount ;
		]
		;
	rdfs:label "fixed lookback strike formula expression"@en ;
	rdfs:comment "QName: NONE:FixedLookbackStrikeFormulaExpression" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	.

fibo-der-der-opt:FixedLookbackStrikeTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-der-opt:LookbackStrikeTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-der-opt:hasFormula ;
			owl:someValuesFrom fibo-der-der-opt:FixedLookbackStrikeFormula ;
		]
		;
	rdfs:label "fixed lookback strike terms"@en ;
	rdfs:comment "QName: NONE:FixedLookbackStrikeTerms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/> ;
	owl:disjointWith fibo-der-der-opt:FloatingLookbackStrikeTerms ;
	skos:definition "Strike terms which reflect the difference between a running maximum value of the observable during the lookback period, and the pre-agreed strike. The agreed settlement is based on the difference between these. Further Notes This is only settled in cash and the strike is predetermined at payoff and the payoff is the max diff between the optimal price and the strike price."@en ;
	.

