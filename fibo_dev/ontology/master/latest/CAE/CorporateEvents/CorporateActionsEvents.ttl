	.

fibo-cae-ce-cae:HolderInitiatedInstruction
	a owl:Class ;
	rdfs:subClassOf
		fibo-cae-ce-cae:InstructionMessage ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-cae-ce-cae:conveys.1 ;
			owl:someValuesFrom fibo-cae-ce-cae:HolderInitiatedAction ;
