	rdfs:label "has price" ;
	rdfs:comment "QName: fibo-fnd-acc-cur:hasPrice" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> ;
	rdfs:range fibo-fnd-acc-cur:Price ;
