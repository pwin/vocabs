	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> ;
	rdfs:range xsd:string ;
	skos:definition "indicates the type of funds, such as next day for US funds" ;
