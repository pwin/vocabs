<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/>
	a owl:Ontology ;
	rdfs:label "Loan Generic Ontology" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:" ;
	dct:abstract "This ontology defines a number of concepts that are not specific to loan that belong in more general FIBO ontologies. They are in the process of being moved over. This ontology will eventually go away." ;
	sm:fileAbbreviation "fibo-loan-lnx-lgn" ;
	owl:imports
		<http://www.omg.org/techprocess/ab/SpecificationMetadata/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/BE/OwnershipAndControl/OwnershipParties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/DebtAndEquities/Debt/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FBC/FunctionalEntities/BusinessRegistries/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/CurrencyAmount/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/AgentsAndPeople/Agents/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Agreements/Agreements/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Agreements/Contracts/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Arrangements/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/ClassificationSchemes/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Arrangements/Documents/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/Occurrences/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Organizations/FormalOrganizations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Parties/Parties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Parties/Roles/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Addresses/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Countries/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Places/Locations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/PaymentsAndSchedules/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/ProductsAndServices/ProductsAndServices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Quantities/QuantitiesAndUnits/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/Values/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/IND/EconomicIndicators/EconomicIndicators/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/IND/InterestRates/InterestRates/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoanExt/LoanGeneric/> ;
	owl:versionInfo "Created with e6Tools Graphical OWL Editor from D:\\SEMANTIC-ARTS\\NAS\\ClientsAndPartners\\EDM Council\\fibo ontologies\\LoanOntology\\loans.vsdx Page:loanUpper" ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Informative ;
	.

fibo-fbc-dae-dbt:hasOutstandingAmount
	a owl:ObjectProperty ;
	fibo-fnd-utl-av:synonym "has actual unpaid balance" ;
	.

fibo-fbc-fct-breg:isSubdivisionOf
	a owl:ObjectProperty ;
	owl:inverseOf fibo-fbc-fct-breg:hasSubdivision ;
	skos:editorialNote "This should be added to the BusinessRegistries ontology" ;
	.

fibo-loan-lnx-lgn:ChargeCategory
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
	rdfs:label "charge cateogry" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:ChargeCategory" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a category indicating what a charge is for (e.g. closing costs, interest)" ;
	fibo-fnd-utl-av:usageNote "Use with LineItem, (has ChargeCategory instance), (hasNumericalValue for number of units) and (hasCost for the amount of money)" ;
	.

fibo-loan-lnx-lgn:ContactInformation
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:onClass fibo-be-le-lp:LegallyCompetentNaturalPerson ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:onClass fibo-fnd-org-fm:FormalOrganization ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:onClass fibo-fnd-plc-adr:PhysicalAddress ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:onClass fibo-loan-lnx-lgn:EmailAddress ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:onClass fibo-loan-lnx-lgn:PhoneNumber ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
			owl:someValuesFrom fibo-loan-lnx-lgn:ContactPurpose ;
		]
		;
	rdfs:label "contact information" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:ContactInformation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a way to make contact with a person or organization for a particular reason regarding something.  The details may be role-specific, so the information cannot always go on the individual or organization" ;
	skos:editorialNote
		"This approach avoids the need to have multiple properties to connect different things to ContactInformation use for different purposes." ,
		"Topic: BE"
		;
	fibo-fnd-utl-av:usageNote "Make sure to check the existing instances of ContactPurpose and use those before creating new ones." ;
	.

fibo-loan-lnx-lgn:ContactPurpose
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-cls:Classifier ;
	rdfs:label "contact purpose" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:ContactPurpose" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a virtual address for communication via phone" ;
	skos:editorialNote "Future consideration: move this property to somewehre in BE." ;
	.

fibo-loan-lnx-lgn:EmailAddress
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-plc-adr:VirtualAddress ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-val:hasDataValue ;
			owl:someValuesFrom xsd:string ;
		]
		;
	rdfs:label "email address" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:EmailAddress" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a virtual address for communication via email" ;
	skos:editorialNote "Future consideration: move this property to Addresses ontology (fibo-fnd-plc-adr)." ;
	.

fibo-loan-lnx-lgn:IndividualContactInformation
	a owl:Class ;
	rdfs:subClassOf fibo-loan-lnx-lgn:ContactInformation ;
	rdfs:label "individual contact information" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:IndividualContactInformation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "contact information for an individual" ;
	.

fibo-loan-lnx-lgn:InformationProduct
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-pas-pas:Product ;
	rdfs:label "information product" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:InformationProduct" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a product that is informational in nature, rather than physical" ;
	skos:editorialNote "Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas)." ;
	.

fibo-loan-lnx-lgn:LineItem
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-lnx-lgn:hasUnitCost ;
			owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-qt-qtu:hasNumericValue ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:decimal ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:has ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-fnd-pas-pas:Product
					fibo-loan-lnx-lgn:ChargeCategory
				) ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-lnx-lgn:hasCost ;
			owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		]
		;
	rdfs:label "line item" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:LineItem" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a description of a product or service including its unit cost, number of units and total cost" ;
	skos:editorialNote "Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas)." ;
	.

fibo-loan-lnx-lgn:OrganizationContactInformation
	a owl:Class ;
	rdfs:subClassOf fibo-loan-lnx-lgn:ContactInformation ;
	rdfs:label "organization contact information" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:OrganizationContactInformation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "contact information for an organization" ;
	.

fibo-loan-lnx-lgn:PaymentHistory
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-doc:Record ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasOutstandingAmount ;
			owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasMember ;
			owl:someValuesFrom fibo-fnd-pas-psch:Payment ;
		]
		;
	rdfs:label "payment history" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:PaymentHistory" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a record of actual payments of principal, interest, and other related amounts made by a borrower to a lender or servicer in order to fulfill their re-payment obligation" ;
	fibo-fnd-utl-av:explanatoryNote "Just like a PaymentSchedule except that the dates and amounts are actuals." ;
	.

fibo-loan-lnx-lgn:PaymentSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:Schedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasMember ;
			owl:someValuesFrom fibo-loan-lnx-lgn:ScheduledPayment ;
		]
		;
	rdfs:label "payment schedule" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:PaymentSchedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a Schedule of amounts of principal and interest that are due over the term of the loan in order to fully pay off the obligation" ;
	skos:editorialNote """Topic: Payments
Not using psch:PaymentSchedule because it uses PaymentEvent which is an OccurrenceKind which does not make sense. Also, it should use hasMember not comprises to put items into a Collection. Ripe for coagulation.""" ;
	.

fibo-loan-lnx-lgn:PhoneNumber
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-plc-adr:VirtualAddress ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-val:hasDataValue ;
			owl:someValuesFrom xsd:string ;
		]
		;
	rdfs:label "phone number" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:PhoneNumber" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a virtual address for communication via phone" ;
	skos:editorialNote "Future consideration: move this property to Addresses ontology (fibo-fnd-plc-adr)." ;
	.

fibo-loan-lnx-lgn:ScheduledPayment
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:AdHocScheduleEntry ,
		fibo-fnd-pas-psch:PaymentObligation ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pas-psch:hasPaymentAmount ;
			owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-lnx-lgn:hasScheduledUnpaidBalance ;
			owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		]
		;
	rdfs:label "scheduled payment" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:ScheduledPayment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "a scheduled payment occurrence that is part of a payment schedule covering the term of the loan" ;
	skos:editorialNote "Topic: Payments" ;
	.

fibo-loan-lnx-lgn:hasContactInformation
	a owl:ObjectProperty ;
	rdfs:label "has contact information" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:hasContactInformation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	rdfs:range fibo-loan-lnx-lgn:ContactInformation ;
	skos:definition "relates an individual or an organization to its contact information" ;
	.

fibo-loan-lnx-lgn:hasCost
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount ;
	rdfs:label "has cost" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:hasCost" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "relates an item to its cost" ;
	skos:editorialNote "Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas)." ;
	fibo-fnd-utl-av:explanatoryNote "This can entail adding up other costs (e.g. 4 units * a unit price)" ;
	.

fibo-loan-lnx-lgn:hasScheduledUnpaidBalance
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:has ;
	rdfs:label "has scheduled unpaid balance" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:hasScheduledUnpaidBalance" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
	skos:definition "relates to what the balance should be after a scheduled payment is made according to contract terms" ;
	skos:editorialNote "Topic: Payments" ;
	.

fibo-loan-lnx-lgn:hasUnitCost
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount ;
	rdfs:label "has unit cost" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:hasUnitCost" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "relates an item to its unit cost" ;
	skos:editorialNote "Future consideration: move this property to ProductsAndServices ontology (fibo-fnd-pas-pas)." ;
	.

fibo-loan-lnx-lgn:isPerformedBy
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-pty-pty:hasParty ;
	rdfs:label "is performed by" ;
	rdfs:comment "QName: fibo-loan-lnx-lgn:isPerformedBy" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/> ;
	skos:definition "relates an occurrence to a party that fully or partially performs the activity or process corresponding to the occurrence (e.g. who performed an inspection)" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.merriam-webster.com/dictionary/perform" ;
	.

fibo-loan-lnx-lgn:worksFor
	a owl:ObjectProperty ;
