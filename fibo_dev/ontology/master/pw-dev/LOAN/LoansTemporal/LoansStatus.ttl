	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	rdfs:range xsd:boolean ;
	.

fibo-loan-tem-sta:delinquent.1
	a owl:DatatypeProperty ;
	rdfs:label "delinquent"@en ;
	rdfs:comment "QName: NONE:delinquent.1" ;
	rdfs:domain fibo-loan-tem-sta:LoanPaymentStatus ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
