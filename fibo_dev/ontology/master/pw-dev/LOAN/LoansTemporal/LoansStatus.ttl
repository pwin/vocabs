@prefix fibo-fnd-utl-val: <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Values/> .
@prefix fibo-loan-loan-loan: <https://spec.edmcouncil.org/fibo/ontology/LOAN/Loans/LoanBasicTerms/> .
@prefix fibo-loan-tem-app: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoanApplicationsTemporal/> .
@prefix fibo-loan-tem-ev: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansEvents/> .
@prefix fibo-loan-tem-sta: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> .
@prefix fibo-loan-typ-stu: <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanTypes/StudentLoans/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/>
	a owl:Ontology ;
	rdfs:label "LoansStatus"@en ;
	rdfs:comment "QName: NONE:" ;
	dct:abstract "This ontology defines the kinds of status that a loan may be in at various stages in its lifecycle, based on the more general temporal concept of statuses. These include the statuses of legal proceedings, of loan performance, of prepayment and of the loan itself e.g. active or in arrears." ;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Accounting/CurrencyAmount/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/InformationExt/TemporalInfo/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/AnnotationVocabulary/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Utilities/Values/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoanTypes/StudentLoans/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/Loans/LoanBasicTerms/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoanApplicationsTemporal/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoansEvents/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/latest/LOAN/LoansTemporal/LoansStatus/> ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Provisional ;
	.

fibo-loan-typ-stu:StudentLoan
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-loan-tem-sta:hasActiveStatus ;
		owl:someValuesFrom fibo-loan-tem-sta:StudentLoanActiveStatus ;
	] ;
	.

fibo-loan-tem-sta:ActiveLoanStatus
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-sta:LoanLifecycleStatus ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-sta:hasLoanApplicationStatus ;
			owl:someValuesFrom fibo-loan-tem-app:LoanApplicationAtDisbursementStage ;
		]
		;
	rdfs:label "active loan status"@en ;
	rdfs:comment "QName: NONE:ActiveLoanStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "Status of a Loan which is in the Disbursement phase."@en ;
	.

fibo-loan-tem-sta:LegalProceedingStatus
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-inf-tem:Status ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isStatusOf ;
			owl:someValuesFrom fibo-loan-tem-ev:LegalProceeding ;
		]
		;
	rdfs:label "legal proceeding status"@en ;
	rdfs:comment "QName: NONE:LegalProceedingStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "The status of some legal proceeding at some point in time."@en ;
	skos:editorialNote "The precise terms for such proceedings will vary on a type of proceeding to type of proceeding basis and so none are articulated here. Review: Maybe in fact there are some common status that apply to all legal proceedings, e.g. None, Imminent, In progress, Closed."@en ;
	.

fibo-loan-tem-sta:LegalProceedingsStatusSelection
	a owl:Class ;
	rdfs:label "legal proceedings status selection"@en ;
	rdfs:comment "QName: NONE:LegalProceedingsStatusSelection" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "The status of any legal proceedings."@en ;
	.

fibo-loan-tem-sta:LoanArrearsStatus
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-sta:LoanPaymentStatus ,
		fibo-loan-tem-sta:LoanSnapshotOrStatus
		;
	rdfs:label "loan arrears status"@en ;
	rdfs:comment "QName: NONE:LoanArrearsStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "Arrears describes the whole period where a loan is either delinquent or in default. [mixed bag of types of Status] formerly Loan Status. Now: The status of arrears performance at any given time in the life of the loan (e.g. present, series of dated past values)."@en ;
	.

fibo-loan-tem-sta:LoanDefaultProceedingStatus
	a owl:Class ;
	rdfs:subClassOf
		fibo-loan-tem-sta:LegalProceedingStatus ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-loan-tem-ev:isStatusOf ;
			owl:someValuesFrom fibo-loan-tem-ev:LoanDefaultProceeding ;
		]
		;
	rdfs:label "loan default proceeding status"@en ;
	rdfs:comment "QName: NONE:LoanDefaultProceedingStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "The status of loan default legal proceeding at some point in time."@en ;
	skos:editorialNote 'Added to enhance Proof of Concept model, which has an aggregation relation from Loan Status to Loan Proceeding. That would not be literally correct as it would make a proceeding (and activity) part of a status. Rather, it must be the status of that proceeding, which is part of this status information set and is reported on at the point in time that the other loan status information may be reported on. Action: Although this "is of" a defined kind of activity called Loan Proceeding, that does not satisfy the need for a dated set of statuses. Need a selection list, based on facts about the Loan Default Proceedings, e.g. Imminent, Pending, Ongoing, Closed. Mabe some of those should be part of a wider list of common terms about all legal proceedings statuses.'@en ;
	.

fibo-loan-tem-sta:LoanLifecycleStatus
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-inf-tem:Status ;
	rdfs:label "loan lifecycle status"@en ;
	rdfs:comment "QName: NONE:LoanLifecycleStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	.

fibo-loan-tem-sta:LoanPayment
	a owl:Class ;
	rdfs:label "loan payment"@en ;
	rdfs:comment "QName: NONE:LoanPayment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition 'Add in here the terms on payment history related terms which are in the "Loan Contract" set.'@en ;
	skos:editorialNote "The above terms all seemed to relate to pre-payment not payment. Do we also need a set of history terms about payment per se? If not, delete this class as prepayment history already modeled."@en ;
	.

fibo-loan-tem-sta:LoanPaymentStatus
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-inf-tem:Status ;
	rdfs:label "loan payment status"@en ;
	rdfs:comment "QName: NONE:LoanPaymentStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	.

fibo-loan-tem-sta:LoanPerformanceStatus
	a owl:Class ;
	rdfs:subClassOf fibo-loan-tem-sta:LoanSnapshotOrStatus ;
	rdfs:label "loan performance status"@en ;
	rdfs:comment "QName: NONE:LoanPerformanceStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "The current performance of the loan, as one of a list of possible statuses."@en ;
	fibo-fnd-utl-av:explanatoryNote "Relates to the performance in terms of whether the customer is paying in time or is in default, and more generally in terms of meeting their obligations in terms of principal or interest payment or any other obligations defined in the Loan Contract (agreement)."@en ;
	.

fibo-loan-tem-sta:LoanPerformanceStatusSelection
	a owl:Class ;
	rdfs:label "loan performance status selection"@en ;
	rdfs:comment "QName: NONE:LoanPerformanceStatusSelection" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	.

fibo-loan-tem-sta:LoanPrepaymentStatus
	a owl:Class ;
	rdfs:subClassOf fibo-loan-tem-sta:LoanSnapshotOrStatus ;
	rdfs:label "loan prepayment status"@en ;
	rdfs:comment "QName: NONE:LoanPrepaymentStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "Information about prepayments of the loan, at a given point in time."@en ;
	skos:editorialNote "This is dated information. Discussion: Payment ahead of the loan Payments ahead of schedule"@en ;
	.

fibo-loan-tem-sta:LoanSnapshotOrStatus
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-inf-tem:Status ;
	rdfs:label "loan snapshot or status"@en ;
	rdfs:comment "QName: NONE:LoanSnapshotOrStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "Information applicable to a loan at a given date."@en ;
	skos:editorialNote "In the model you have Loan Balance, Loan DTI, ALSO ADD: Loan Default Proceeding should be referenced from Loan Snapshot as well. Combine Balance and Status, leave Rate and ?? out..."@en ;
	.

fibo-loan-tem-sta:LoanStatus
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-inf-tem:Status ;
	rdfs:label "loan status"@en ;
	rdfs:comment "QName: NONE:LoanStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "Statuses which occur for a period of time, relating to a loan."@en ;
	fibo-fnd-utl-av:explanatoryNote "The status applies at a period of time, and is applicable across all phases."@en ;
	.

fibo-loan-tem-sta:StudentLoanActiveStatus
	a owl:Class ;
	rdfs:subClassOf fibo-loan-tem-sta:ActiveLoanStatus ;
	rdfs:label "student loan active status"@en ;
	rdfs:comment "QName: NONE:StudentLoanActiveStatus" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	skos:definition "The status of the Student Loan at a given point in time, during the phase where it is active (the disbursement phase)."@en ;
	.

fibo-loan-tem-sta:StudentLoanActiveStatusSelection
	a owl:Class ;
	rdfs:label "student loan active status selection"@en ;
	rdfs:comment "QName: NONE:StudentLoanActiveStatusSelection" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	.

fibo-loan-tem-sta:allocationToFees
	a owl:ObjectProperty ;
	rdfs:label "allocation to fees"@en ;
	rdfs:comment "QName: NONE:allocationToFees" ;
	rdfs:domain fibo-loan-tem-sta:LoanPayment ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansTemporal/LoansStatus/> ;
	rdfs:range fibo-fnd-acc-cur:MonetaryAmount ;
	skos:definition "The amount of the payment which is allocated to other fees."@en ;
	.

