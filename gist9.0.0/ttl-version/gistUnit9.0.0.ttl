###  https://ontologies.semanticarts.com/gist/MoleUnit
gist:MoleUnit rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( gist:SimpleUnitOfMeasure
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gist:hasBaseUnit ;
                                                           owl:hasValue gist:_mole
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Amount of chemical material.  Measured in Avogadro units (moles) of 6.02 x 10^23 molecules."^^xsd:string ;
              rdfs:label "Mole Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/RatioUnit
gist:RatioUnit rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( gist:UnitOfMeasure
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gist:denominator ;
                                                            owl:someValuesFrom gist:UnitOfMeasure
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gist:numerator ;
                                                            owl:someValuesFrom gist:UnitOfMeasure
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:comment "A UnitOfMeasure composed of a numerator unit and a denominator unit."^^xsd:string ,
                            "EXAMPLE:  Miles per hour."^^xsd:string ,
                            "NOTE: If needed, a conversion factor for a RatioUnit can be (recursively) derived from the conversion factors of the numerator and denominator units.  E.g., the derived conversion factor from km/minute to meters/second is 1000/60 or 16 2/3."^^xsd:string ;
               rdfs:label "Ratio Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/SimpleUnitOfMeasure
gist:SimpleUnitOfMeasure rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/TemperatureUnit
