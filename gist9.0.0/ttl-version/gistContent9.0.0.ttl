###  https://ontologies.semanticarts.com/gist/Address
gist:Address rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/Category
gist:Category rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/Content
gist:Content rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/ContentExpression
gist:ContentExpression rdf:type owl:Class ;
                       rdfs:subClassOf gist:Content ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty gist:categorizedBy ;
                                         owl:someValuesFrom gist:GeneralMediaType
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty gist:expressedIn ;
                                         owl:someValuesFrom gist:Language
                                       ] ;
                       rdfs:comment "Intellectual Property reduced to text, audio etc.  If it contains text (written or spoken), it may be in a language."^^xsd:string ;
                       rdfs:label "Content Expression"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/FormattedContent
gist:FormattedContent rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( gist:ContentExpression
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty gist:expressedIn ;
                                                                   owl:someValuesFrom gist:MimeType
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:comment "Content which is in a particular format. (E.g., html, pdf, jpg.)"^^xsd:string ;
                      rdfs:label "Formatted Content"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/GeneralMediaType
gist:GeneralMediaType rdf:type owl:Class ;
                      rdfs:subClassOf gist:Category ;
                      rdfs:comment "EXAMPLE: audio, still image, video, textual, physical (e.g., a statue), or performance (i.e. a play).  Or it could be oil or pastel for a painting."^^xsd:string ,
                                   "The real-world media type for content."^^xsd:string ;
                      rdfs:label "General Media Type"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Medium
gist:Medium rdf:type owl:Class ;
            rdfs:subClassOf gist:Category ;
            rdfs:comment "A physicality on which a work could be implemented or exposed. E.g., paper, clay, or a computer monitor."^^xsd:string ;
            rdfs:label "Medium"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Message
gist:Message rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( gist:ContentExpression
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gist:fromAgent ;
                                                          owl:someValuesFrom [ rdf:type owl:Class ;
                                                                               owl:unionOf ( gist:Address
                                                                                             gist:Organization
                                                                                             gist:Person
                                                                                           )
                                                                             ]
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty gist:toAgent ;
                                                          owl:someValuesFrom [ rdf:type owl:Class ;
                                                                               owl:unionOf ( gist:Address
                                                                                             gist:Organization
                                                                                             gist:Person
                                                                                           )
                                                                             ]
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "A specific instance of content sent from an Organization, Person, or Application to at least one other Organization, Person, or Application."^^xsd:string ,
                          "EXAMPLES: An email message, a phone call, a voice message, or a Web Service message."^^xsd:string ;
             rdfs:label "Message"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/MimeType
gist:MimeType rdf:type owl:Class ;
              rdfs:subClassOf gist:Category ;
              rdfs:comment "A digitized type that computer applications can recognize."^^xsd:string ;
              rdfs:label "MIME Type"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Organization
gist:Organization rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/Person
gist:Person rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/RenderedContent
gist:RenderedContent rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( gist:ContentExpression
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty gist:expressedIn ;
                                                                  owl:someValuesFrom gist:MimeType
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty gist:renderedOn ;
                                                                  owl:someValuesFrom gist:Medium
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:comment "Content which has been expressed, either to print, or through speakers, or on a monitor."^^xsd:string ;
                     rdfs:label "Rendered Content"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Text
gist:Text rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( gist:Content
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty gist:expressedIn ;
                                                       owl:someValuesFrom gist:Language
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty gist:containedText ;
                                                       owl:someValuesFrom xsd:string
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:comment "Content expressed as words and numbers (not graphics)."^^xsd:string ;
          rdfs:label "Text"^^xsd:string .


