#################################################################

###  https://ontologies.semanticarts.com/gist/about
gist:about rdf:type owl:ObjectProperty ;
           owl:inverseOf gist:describedIn ;
           rdfs:domain gist:Content ;
           rdfs:comment "Subject matter of a document."^^xsd:string ;
           rdfs:label "About"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/categorizedBy
gist:categorizedBy rdf:type owl:ObjectProperty .


###  https://ontologies.semanticarts.com/gist/describedIn
gist:describedIn rdf:type owl:ObjectProperty ;
                 rdfs:comment "Docuemnt the subject matter appeared in"^^xsd:string ;
                 rdfs:label "Described In"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/expressedIn
gist:expressedIn rdf:type owl:ObjectProperty ;
                 rdfs:comment "The language something was expressed in"^^xsd:string ;
                 rdfs:label "Expressed In"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/fromAgent
gist:fromAgent rdf:type owl:ObjectProperty ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( gist:Address
                                          gist:Organization
                                          gist:Person
                                        )
                          ] ;
               rdfs:comment "The source of a message or shipment"^^xsd:string ;
               rdfs:label "From Agent"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/renderedOn
gist:renderedOn rdf:type owl:ObjectProperty ;
                rdfs:comment "What media somethign was rendered On"^^xsd:string ;
                rdfs:label "Rendered On"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/toAgent
gist:toAgent rdf:type owl:ObjectProperty ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( gist:Address
                                        gist:Organization
                                        gist:Person
                                      )
                        ] ;
             rdfs:comment "Comment: this is not the inverse of fromAgent. A message can be from someone. If we made it the inverse the person would be \"to\" the message"^^xsd:string ;
             rdfs:label "To Agent"^^xsd:string .


