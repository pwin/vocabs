#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/license
gist:license rdf:type owl:AnnotationProperty ;
             rdfs:comment "An annotation for providing the licensing on this or derrivative ontologies"^^xsd:string .


#################################################################
#    Object Properties
#################################################################

###  https://ontologies.semanticarts.com/gist/actual
gist:actual rdf:type owl:ObjectProperty ;
            rdfs:range gist:TimeInstant ;
            rdfs:comment "When something did occur, therefore noting an historical event."^^xsd:string ;
            rdfs:label "Actual"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/actualEnd
gist:actualEnd rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf gist:actual ,
                                  gist:end ;
               rdfs:comment "When something did end, therefore noting an historical event."^^xsd:string ;
               rdfs:label "Actual End"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/actualStart
gist:actualStart rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf gist:actual ,
                                    gist:start ;
                 rdfs:comment "When something did start, therefore noting an historical event."^^xsd:string ;
                 rdfs:label "Actual Start"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/allocatedBy
gist:allocatedBy rdf:type owl:ObjectProperty ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( gist:IntellectualProperty
                                            gist:Organization
                                            gist:Person
                                          )
                            ] ;
                 rdfs:comment "Connection between an ID and the thing that minted the ID.  It may be a person or organization, or could be an algorithm (next available or random number generator)"^^xsd:string ;
                 rdfs:label "Allocated By"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/basedOn
gist:basedOn rdf:type owl:ObjectProperty ;
             rdfs:comment "pointer to the thing something was derived from"^^xsd:string ;
             rdfs:label "Based On"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/categorizedBy
gist:categorizedBy rdf:type owl:ObjectProperty ;
                   rdfs:comment "Points to a taxonomy item or other less formally defined class."^^xsd:string ;
                   rdfs:label "Categorized By"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/characterizedAs
gist:characterizedAs rdf:type owl:ObjectProperty ;
                     rdfs:domain gist:Event ;
                     rdfs:range gist:Behavior ;
                     rdfs:comment "A way to categorized a behavior"^^xsd:string ;
                     rdfs:label "Characterized As"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/directPartOf
gist:directPartOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf gist:hasDirectPart ;
                  rdfs:comment "The relationship between a  part and a whole where the part has independent existence."^^xsd:string ;
                  rdfs:label "Direct Part Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/end
gist:end rdf:type owl:ObjectProperty ;
         rdfs:range gist:TimeInstant ;
         rdfs:comment "Connects the subject to its end time."^^xsd:string ;
         rdfs:label "End"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/governedBy
gist:governedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf gist:governs ;
                rdfs:comment "A reference from the thing being governed to the governor"^^xsd:string ;
                rdfs:label "Governed By"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/governs
gist:governs rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( gist:Intention
                                         gist:Organization
                                         gist:Person
                                         gist:Template
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( gist:Category
                                        gist:Content
                                        gist:GeoRegion
                                        gist:IntellectualProperty
                                        gist:Intention
                                        gist:Organization
                                        gist:Person
                                        gist:PhysicalIdentifiableItem
                                        gist:PhysicalSubstance
                                      )
                        ] ;
             rdfs:comment "The subject controls or inhibits the object in some way"^^xsd:string ;
             rdfs:label "Governs"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasAltitude
gist:hasAltitude rdf:type owl:ObjectProperty ;
                 rdfs:domain gist:GeoPoint ;
                 rdfs:range gist:Extent ;
                 rdfs:comment "Distance above sea level"^^xsd:string ;
                 rdfs:label "Has Altitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasBaseUnit
gist:hasBaseUnit rdf:type owl:ObjectProperty ;
                 rdfs:domain gist:UnitOfMeasure ;
                 rdfs:range gist:BaseUnit ;
                 rdfs:comment "EXAMPLE: saying that a furlong hasBaseUnit  meter says it is a DistanceUnit."^^xsd:string ,
                              "Relates a UnitOfMeasure to its BaseUnit.  This indicates what kind Unit something is, e.g. saying that a furlong hasBaseUnit  meter says it is a DistanceUnit."^^xsd:string ;
                 rdfs:label "Has Base Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasBirthDate
gist:hasBirthDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:actualStart ;
                  rdfs:domain gist:LivingThing ;
                  rdfs:range gist:TimeInstant ;
                  rdfs:comment "Date a living thing was \"born\" (or germinated, for plants)."^^xsd:string ;
                  rdfs:label "Has Birthdate"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDeathDate
gist:hasDeathDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:actualEnd ;
                  rdfs:domain gist:LivingThing ;
                  rdfs:range gist:TimeInstant ;
                  rdfs:comment "Date a living thing died"^^xsd:string ;
                  rdfs:label "Has Death Date"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDirectPart
gist:hasDirectPart rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf gist:hasPart ;
                   rdfs:comment "NOTE: No cascading delete."^^xsd:string ,
                                "NOTE: Use this property to directly associate parts. hasPart is the transitive version."^^xsd:string ,
                                "The relationship between a whole and a part where the part has independent existence."^^xsd:string ;
                   rdfs:label "Has Direct Part"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasMagnitude
gist:hasMagnitude rdf:type owl:ObjectProperty ;
                  rdfs:range gist:Magnitude ;
                  rdfs:comment "To have a comparable numerical value. Each magnitude has a unit."^^xsd:string ;
                  rdfs:label "Has Magnitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasMember
gist:hasMember rdf:type owl:ObjectProperty ;
               owl:inverseOf gist:memberOf ;
               rdfs:domain gist:Collection ;
               rdfs:comment "Relates a Collection to its member individuals."^^xsd:string ;
               rdfs:label "Has Member"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPart
gist:hasPart rdf:type owl:ObjectProperty ;
             owl:inverseOf gist:partOf ;
             rdf:type owl:TransitiveProperty ;
             rdfs:comment "The transitive version of hasDirectPart"^^xsd:string ;
             rdfs:label "Has Part"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPrecision
gist:hasPrecision rdf:type owl:ObjectProperty ;
                  rdfs:range gist:Magnitude ;
                  rdfs:comment "EXAMPLE:  Temperature precise to tenth of a degree C; TimeInstant precise to 24 hours."^^xsd:string ,
                               "Links a Magnitude to the degree of accuracy of the numeric value.   This allows for fuzzy numbers.  All magnitudes have a precision.  Usually we don't record them.  When we do this, it will be a value whose extent covers 2 standard deviations around the stated magnitude"^^xsd:string ,
                               "NOTE:  Most frequently apples to Magnitude(s) and TimeInstant. Could also apply to a measurement."^^xsd:string ;
                  rdfs:label "Has Precision"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasUoM
gist:hasUoM rdf:type owl:ObjectProperty ;
            rdfs:domain gist:Magnitude ;
            rdfs:range gist:UnitOfMeasure ;
            rdfs:comment "Which unit of measure you are using. All measures are in some uom, even if we don't know what it is initially."^^xsd:string ;
            rdfs:label "Has Unit of Measure"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/identifiedBy
gist:identifiedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf gist:identifies ;
                  rdf:type owl:InverseFunctionalProperty ;
                  rdfs:range gist:ID ;
                  rdfs:comment "This is like a uri: a thing can have more than one ID, but each of the IDs must refer to a unique thing."^^xsd:string ;
                  rdfs:label "Identified By"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/identifies
gist:identifies rdf:type owl:ObjectProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:comment "The thing the identifier is referringo"^^xsd:string ;
                rdfs:label "Identifies"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/madeUpOf
gist:madeUpOf rdf:type owl:ObjectProperty ;
              rdfs:range gist:PhysicalSubstance ;
              rdfs:comment "as in the vase is made up of clay"^^xsd:string ;
              rdfs:label "Made Up Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/memberOf
gist:memberOf rdf:type owl:ObjectProperty ;
              rdfs:domain gist:Collection ;
              rdfs:comment "What group the member is in"^^xsd:string ;
              rdfs:label "Member Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/multiplicand
gist:multiplicand rdf:type owl:ObjectProperty ;
                  rdfs:domain gist:ProductUnit ;
                  rdfs:range gist:UnitOfMeasure ;
                  rdfs:comment "Relates a ProductUnit such as square mile to the second of two units multiplied together (e.g. mile)."^^xsd:string ;
                  rdfs:label "Multiplicand"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/multiplier
gist:multiplier rdf:type owl:ObjectProperty ;
                rdfs:domain gist:ProductUnit ;
                rdfs:range gist:UnitOfMeasure ;
                rdfs:comment "Relates a ProductUnit such as square mile to the first of two units multiplied together (e.g. mile)"^^xsd:string ;
                rdfs:label "Multiplier"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/offspringOf
gist:offspringOf rdf:type owl:ObjectProperty ;
                 rdfs:domain gist:LivingThing ;
                 rdfs:range gist:LivingThing ;
                 rdfs:comment "Biological offspring"^^xsd:string ;
                 rdfs:label "Offspring Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/owns
gist:owns rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( gist:Organization
                                      gist:Person
                                    )
                      ] ;
          rdfs:comment "Possessinng and controlling.  Ultimate form of ownership is the right to destroy.  Long list of potential Range classes"^^xsd:string ;
          rdfs:label "Owns"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/parentOf
gist:parentOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Biological Parent"^^xsd:string ;
              rdfs:label "Parent Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/partOf
gist:partOf rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:comment "The transitive version of directPartOf"^^xsd:string ;
            rdfs:label "Part Of"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/produces
gist:produces rdf:type owl:ObjectProperty ;
              rdfs:comment "EXAMPLE: a task produces a deliverable."^^xsd:string ,
                           "The subject creates the object."^^xsd:string ;
              rdfs:label "Produces"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/start
gist:start rdf:type owl:ObjectProperty ;
           rdfs:range gist:TimeInstant ;
           rdfs:comment "Connects the subject to its start time."^^xsd:string ;
           rdfs:label "Start"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/triggeredBy
gist:triggeredBy rdf:type owl:ObjectProperty ;
                 rdfs:comment "general causal relation.  For obligations a property that describes what would happen to trigger the contingent obligation.  In most cases, before the Contingent becomes an Obligation, the triggered by event is a planned event (that is it hasn't happened yet -- if it had happened the contingency would no longer be contingent.  In most cases it will be  a ContingentEvent .  Otehr uses include controls, processes etc"^^xsd:string ;
                 rdfs:label "Triggered By"^^xsd:string .


#################################################################
#    Data properties
#################################################################

###  https://ontologies.semanticarts.com/gist/conversionOffset
gist:conversionOffset rdf:type owl:DatatypeProperty ;
                      rdfs:domain gist:UnitOfMeasure ;
                      rdfs:range xsd:double ;
                      rdfs:comment "Add this number to get to the zero point.  On the Celsius scale, the conversionOffset is -273.15 degrees C. On the Fahrenheit scale it is -459.67 degrees.  Is equal to 0 when the unit has the same zero point as the base unit. e.g. inch, meter."^^xsd:string ;
                      rdfs:label "Conversion Offset"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/convertToBase
gist:convertToBase rdf:type owl:DatatypeProperty ;
                   rdfs:domain gist:UnitOfMeasure ;
                   rdfs:range xsd:double ;
                   rdfs:comment """The conversion factor used to get to the base unit.  E.g., multiplying by 0.0254 gets you from inches to meters. Divide by this number to go the other way. Used in conjunction with conversionOffset to convert from one unit to another. 
Degrees K =  (Degrees F - conversionOffset) * convertToBase. Or K = (F-(-469.67)) * (5/9).  To go the other way: F = (K * 9/5) -469.67.  Try it on Google."""^^xsd:string ;
                   rdfs:label "ConvertToBase"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/decimalValue
gist:decimalValue rdf:type owl:DatatypeProperty ;
                  rdfs:domain gist:Magnitude ;
                  rdfs:range xsd:double ;
                  rdfs:comment "The actual value of a magnitude."^^xsd:string ;
                  rdfs:label "Decimal Value"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/epoch
gist:epoch rdf:type owl:DatatypeProperty ;
           rdfs:domain gist:TimeInstant ;
           rdfs:range xsd:double ;
           rdfs:comment "seconds since 1/1/1970 UTC (unix time )"^^xsd:string ;
           rdfs:label "Epoch"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/latitude
gist:latitude rdf:type owl:DatatypeProperty ;
              rdfs:domain gist:GeoPoint ;
              rdfs:range xsd:double ;
              rdfs:comment "degrees above or below equator"^^xsd:string ;
              rdfs:label "Latitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/localDate
gist:localDate rdf:type owl:DatatypeProperty ;
               rdfs:domain gist:TimeInstant ;
               rdfs:comment "LD"^^xsd:string ;
               rdfs:label "Local Date"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/localDateTime
gist:localDateTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain gist:TimeInstant ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "LDT"^^xsd:string ;
                   rdfs:label "Local Date Time"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/localTime
gist:localTime rdf:type owl:DatatypeProperty ;
               rdfs:domain gist:TimeInstant ;
               rdfs:comment "LT"^^xsd:string ;
               rdfs:label "Local Time"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/longitude
gist:longitude rdf:type owl:DatatypeProperty ;
               rdfs:domain gist:GeoPoint ;
               rdfs:range xsd:double ;
               rdfs:comment "ddegrees from GM"^^xsd:string ;
               rdfs:label "Longitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/name
gist:name rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "Relates an individual to a casual name.  NOTE: For more formal use, consider using a sub property of the object property, identifiedBy."^^xsd:string ;
          rdfs:label "Name"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/sequence
gist:sequence rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:integer ;
              rdfs:comment "For ordering ordered lists."^^xsd:string ;
              rdfs:label "Sequence"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/uniqueText
gist:uniqueText rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:range xsd:string ;
                rdfs:comment "This is used for the actual value of a key or ID where you don't want the possibility of having more than one."^^xsd:string ;
                rdfs:label "Unique Text"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/universalDate
gist:universalDate rdf:type owl:DatatypeProperty ;
                   rdfs:domain gist:TimeInstant ;
                   rdfs:comment "UD"^^xsd:string ;
                   rdfs:label "Universal Date"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/universalDateTime
gist:universalDateTime rdf:type owl:DatatypeProperty ;
                       rdfs:domain gist:TimeInstant ;
                       rdfs:range xsd:dateTime ;
                       rdfs:comment "UDT"^^xsd:string ;
                       rdfs:label "Universal Date Time"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/universalTime
gist:universalTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain gist:TimeInstant ;
                   rdfs:comment "UT"^^xsd:string ;
                   rdfs:label "Universal Time"^^xsd:string .


#################################################################
#    Classes
#################################################################

###  https://ontologies.semanticarts.com/gist/Area
gist:Area rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( gist:Magnitude
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty gist:hasUoM ;
                                                       owl:someValuesFrom gist:AreaUnit
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:comment "A measurement of two-dimensional space."^^xsd:string ;
          rdfs:label "Area"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/AreaUnit
gist:AreaUnit rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( gist:ProductUnit
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gist:multiplicand ;
                                                           owl:someValuesFrom gist:DistanceUnit
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gist:multiplier ;
                                                           owl:someValuesFrom gist:DistanceUnit
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "A unit of two-dimensional area, such as square inches or hectares."^^xsd:string ;
              rdfs:label "Area Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/BaseUnit
gist:BaseUnit rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:oneOf ( gist:_USDollar
                                                gist:_ampere
                                                gist:_bit
                                                gist:_candela
                                                gist:_each
                                                gist:_kelvin
                                                gist:_kilogram
                                                gist:_meter
                                                gist:_mole
                                                gist:_second
                                              )
                                  ] ;
              rdfs:subClassOf gist:SimpleUnitOfMeasure ;
              rdfs:comment "A primitive unit that cannot be decomposed into other units. It can be converted from one measurement system to another.  The base units in gist are the seven primitive units from the System Internationale (SI): (meter, second, kilogram, ampere, kelvin, mole, candela), plus three convenience ones: each. bit and usDollar."^^xsd:string ;
              rdfs:label "Base Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Behavior
gist:Behavior rdf:type owl:Class ;
              rdfs:subClassOf gist:Category ;
              rdfs:comment "A way of categorizing events.  E.g., differentiating drilling versus cutting."^^xsd:string ;
              rdfs:label "Behavior"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Category
gist:Category rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty gist:allocatedBy ;
                                owl:someValuesFrom [ rdf:type owl:Class ;
                                                     owl:unionOf ( gist:IntellectualProperty
                                                                   gist:Organization
                                                                   gist:Person
                                                                 )
                                                   ]
                              ] ;
              rdfs:comment "A concept or label used to categorize other instances informally. Things that can be thought of as types are usually Categories."^^xsd:string ,
                           "EXAMPLE: Tags used in folksonomies; formal definitions from other systems."^^xsd:string ,
                           "NOTE: Often a 'bucket' can be modeled either as an owl:Class or as a gist:Category. Use the latter if you don't care much about the formal structure of the different types, or if there is a whole hierarchy of types that are going to be managed by a group separate from the ontology developers. The formal structure may be defined elsewhere and linked to, if necessary."^^xsd:string ;
              rdfs:label "Category"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Collection
gist:Collection rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty gist:hasMember ;
                                  owl:someValuesFrom owl:Thing
                                ] ;
                rdfs:comment "Any identifiable grouping of instances.  For instance, a jury is a collection of people."^^xsd:string ,
                             "EXAMPLES: A jury is a group of people,   a financial ledger is a collection of transaction entries; a route is an (ordered) collection of segments."^^xsd:string ;
                rdfs:label "Collection"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Content
gist:Content rdf:type owl:Class ;
             owl:disjointWith gist:GeoPoint ,
                              gist:GeoRegion ,
                              gist:Organization ,
                              gist:PhysicalIdentifiableItem ,
                              gist:PhysicalSubstance ,
                              gist:TimeInstant ,
                              gist:UnitOfMeasure ;
             rdfs:comment "A document, program, image, etc.  (Categories are not content until they are written down.)"^^xsd:string ;
             rdfs:label "Content"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/DistanceUnit
gist:DistanceUnit rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( gist:SimpleUnitOfMeasure
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty gist:hasBaseUnit ;
                                                               owl:hasValue gist:_meter
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:comment "A unit to measure linear distance, such as feet or kilometers."^^xsd:string ;
                  rdfs:label "Distance Unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Duration
gist:Duration rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( gist:Magnitude
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gist:hasUoM ;
                                                           owl:someValuesFrom gist:DurationUnit
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "EXAMPLE: One week (or seven days), but not Jan 1, 2008 to Jan 7, 2008 (which is an interval). Intervals have durations, but are not themselves durations."^^xsd:string ,
                           "Time, but not on a timeline."^^xsd:string ;
              rdfs:label "Duration"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/DurationUnit
gist:DurationUnit rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( gist:SimpleUnitOfMeasure
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty gist:hasBaseUnit ;
                                                               owl:hasValue gist:_second
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:comment "A unit to measure passage of time: hours, days, years."^^xsd:string ;
                  rdfs:label "Duration Unit"^^xsd:string .


