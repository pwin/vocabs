                                                 owl:versionIRI <https://ontologies.semanticarts.com/o/gistTop9.0.0> ;
                                                 rdfs:comment "All the high level gist classes and key relastionships.  Most disjoints are declared at this level"^^xsd:string ,
                                                              "gistTop"^^xsd:string ;
