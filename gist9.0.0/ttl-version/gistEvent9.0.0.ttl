###  https://ontologies.semanticarts.com/gist/ContemporaneousEvent
gist:ContemporaneousEvent rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( gist:Event
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty gist:actualStart ;
                                                                       owl:someValuesFrom gist:TimeInstant
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty gist:actualEnd ;
                                                                       owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                                                       owl:onClass gist:TimeInstant
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:comment "An event that actually started after the present time.  When we record an end time it ceases to be contemperaneous"^^xsd:string ,
                                       "NOTE: All contemporaneous events eventually end and, due to the nature of the open world, we can never be sure that a contemporaneous event hasn't ended. As a result, this is really a contemporaneous and historical event."^^xsd:string ;
                          rdfs:label "Contemporaneous Event"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/ContingentEvent
gist:ContingentEvent rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( gist:Event
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty gist:hasMagnitude ;
                                                                  owl:someValuesFrom gist:Percentage
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty gist:plannedEnd ;
                                                                  owl:someValuesFrom gist:TimeInstant
                                                                ]
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty gist:plannedStart ;
                                                                  owl:someValuesFrom gist:TimeInstant
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:comment "An event with a probability of happening in the future, and usually dependent upon some other event or condition."^^xsd:string ;
                     rdfs:label "Contingent Event"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/Event
gist:Event rdf:type owl:Class .


###  https://ontologies.semanticarts.com/gist/HistoricalEvent
gist:HistoricalEvent rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( gist:Event
                                                                [ rdf:type owl:Restriction ;
