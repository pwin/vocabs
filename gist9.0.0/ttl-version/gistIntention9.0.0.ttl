gist:prevents rdf:type owl:ObjectProperty ;
              rdfs:domain gist:Intention ;
              rdfs:range gist:Behavior ;
              rdfs:comment "The intention (say a law) is intended to prevent this kind of behavior (say jaywalking)"^^xsd:string ;
              rdfs:label "Prevents"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/requires
gist:requires rdf:type owl:ObjectProperty ;
              rdfs:domain gist:Intention ;
              rdfs:range gist:Behavior ;
              rdfs:comment "An intention that sets out a state of satisfaction (you are required to drive on right side of the road)"^^xsd:string ;
              rdfs:label "Requires"^^xsd:string .


